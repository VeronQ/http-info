{
	"compilerOptions": {
		"declaration": true,
		"importHelpers": true,
		"strict": true,
		"target": "es5",
		"module": "commonjs",
		"resolveJsonModule": true,
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts"
	]
}
