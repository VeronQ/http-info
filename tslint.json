{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs",
			2
		],
		"no-console": false,
		"object-literal-sort-keys": false
	},
	"rulesDirectory": [],
	"linterOptions": {
		"exclude": [
			"dist/*",
			"test/*"
		]
	}
}
